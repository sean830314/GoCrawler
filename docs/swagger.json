    "paths": {
        "/dcard/list-boards": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "summary": "List Dcard Boards",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/app.Response"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/app.Response"
                        }
                    }
                }
            }
        },
        "/dcard/save-articles": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "summary": "Download Article",
                "parameters": [
                    {
                        "type": "string",
                        "description": "BoardID",
                        "name": "board_id",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "integer",
                        "description": "num of article",
                        "name": "num_article",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/app.Response"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/app.Response"
                        }
                    }
                }
            }
        },
        "/ping": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "summary": "Get Ping",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/app.Response"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/app.Response"
                        }
                    }
                }
            }
        },
        "/ptt/save-articles": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "summary": "Download Article",
                "parameters": [
                    {
                        "type": "string",
                        "description": "BoardName",
                        "name": "board",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "integer",
                        "description": "num of page",
                        "name": "num_page",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/app.Response"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/app.Response"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "app.Response": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer"
                },
                "data": {
                    "type": "object"
                },
                "msg": {
                    "type": "string"
                }
            }
        }
    },
